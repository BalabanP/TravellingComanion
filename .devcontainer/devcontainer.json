	"name": "Python 3",
	"dockerComposeFile": "../docker/app/docker-compose.yml",
	"service": "app-container",
	"workspaceFolder": "/workspaces",
	// Add the IDs of extensions you want installed when the container is created.
	"extensions": [
		"ms-python.python",
		"ms-python.vscode-pylance"
	],

}
